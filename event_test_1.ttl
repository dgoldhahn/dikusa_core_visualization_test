prefix : <http://www.werkd.saw-leipzig.de/>
prefix werkd: <http://www.werkd.saw-leipzig.de/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>


:event
    :has_location :location ;
    :is_part_of_event :event ;
    :has_subsequent_event :event ;
    :has_participant :participant ;
    :has_resource_relation :event_resource_relation .

:location
    :has_place :place ;
    :has_begin "begin earliest/latest" ;
    :has_end "end earliest/latest" .

:participant
    :has_begin "begin earliest/latest" ;
    :has_end "end earliest/latest" ;
    :has_participant_value :person ;
    :has_participant_role :participant_role .

:resource
    :has_event_relation :event_resource_relation .

:work  rdfs:subClassOf :resource .

:object  rdfs:subClassOf :resource .

:event_resource_relation 
    :has_type :type ;
    :has_begin "begin earliest/latest" ;
    :has_end "end earliest/latest" .
