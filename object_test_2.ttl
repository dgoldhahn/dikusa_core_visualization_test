prefix : <http://www.werkd.saw-leipzig.de/>
prefix werkd: <http://www.werkd.saw-leipzig.de/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>


:object
    :has_actor_relation :object_actor_relation ;
    :is_part_of :object

:actor
    :has_object_relation :object_actor_relation .

:person rdfs:subClassOf :actor .
:group rdfs:subClassOf :actor .

:object_actor_relation
    :has_begin "begin earliest/latest" ;
    :has_end "end earliest/latest" ;
    :has_type :type .



:object_creation :has_creation_type :creation_type .

:object_destruction rdfs:subClassOf :event .
:object_destruction :has_destruction_type :destruction_type .

:object_modification rdfs:subClassOf :event .
:object_modification :has_modification_type :modification_type .
